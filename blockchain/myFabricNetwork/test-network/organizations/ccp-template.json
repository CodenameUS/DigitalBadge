{
  "name": "test-network-${ORG}",
  "version": "1.0.0",
  "client": {
    "organization": "${ORG}Org",
    "connection": {
      "timeout": {
        "peer": {
          "endorser": "300"
        }
      }
    }
  },
  "organizations": {
    "${ORG}Org": {
      "mspid": "${ORG}MSP",
      "peers": ["peer0.${ORG}.libBadge.com"],
      "certificateAuthorities": ["ca.${ORG}.libBadge.com"]
    }
  },
  "peers": {
    "peer0.${ORG}.libBadge.com": {
      "url": "grpcs://localhost:${P0PORT}",
      "tlsCACerts": {
        "pem": "${PEERPEM}"
      },
      "grpcOptions": {
        "ssl-target-name-override": "peer0.${ORG}.libBadge.com",
        "hostnameOverride": "peer0.${ORG}.libBadge.com"
      }
    }
  },
  "certificateAuthorities": {
    "ca.${ORG}.libBadge.com": {
      "url": "https://localhost:${CAPORT}",
      "caName": "ca-${ORG}",
      "tlsCACerts": {
        "pem": ["${CAPEM}"]
      },
      "httpOptions": {
        "verify": false
      }
    }
  }
}
